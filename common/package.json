{
  "name": "@fs/ppaas-common",
  "version": "3.0.0",
  "description": "Common Code for the PewPewController and PewPewAgent",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**/*",
    "dist/types/**/*"
  ],
  "scripts": {
    "build": "npm run lint && npm run buildonly",
    "buildonly": "tsc",
    "test": "npm run linterror && npm run buildonly && nyc mocha ./dist/test --timeout 30000 -r dotenv-flow/config",
    "testonly": "nyc mocha ./dist/test --timeout 30000 -r dotenv-flow/config",
    "integration": "npm run build && nyc mocha ./dist/integration --timeout 300000 -r dotenv-flow/config",
    "coverage": "npm run build && nyc mocha ./dist/test ./dist/integration --timeout 300000 -r dotenv-flow/config",
    "prepare": "npm run buildonly && husky install",
    "publish": "npmPublish",
    "lint": "eslint --fix . --ext .ts",
    "linterror": "eslint . --ext .ts --max-warnings 0",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fs-eng/ppaas-common.git"
  },
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  },
  "nyc": {
    "exclude": "**/*.spec.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.363.0",
    "@aws-sdk/client-s3": "^3.363.0",
    "@aws-sdk/client-sqs": "^3.363.0",
    "@aws-sdk/lib-storage": "^3.363.0",
    "@fs/config-wasm": "file:../lib/config-wasm/pkg",
    "bunyan": "~1.8.0"
  },
  "devDependencies": {
    "@aws-sdk/util-stream-node": "^3.363.0",
    "@fs/npm-publisher": "^1.5.1",
    "@types/bunyan": "~1.8.8",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-sdk-client-mock": "^3.0.0",
    "chai": "^4.3.7",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.40.0",
    "husky": "~8.0.3",
    "mocha": "^10.2.0",
    "nyc": "~15.1.0",
    "typescript": "~5.2.0"
  }
}
